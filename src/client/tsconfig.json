{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2019",
    "lib": ["dom"],
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "allowJs": false,
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strictFunctionTypes": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "rootDir": "./"
  },
}
